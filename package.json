{
  "name": "natally-nuxt",
  "version": "3.1.9",
  "scripts": {
    "bootstrap:component": "ts-node ./bootstrap.component.ts",
    "deploy": "npm run generate && ts-node ./deploy.ts",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint --ignore-path .gitignore ./**/*.{js,ts,vue}",
    "lint:style": "stylelint --ignore-path .gitignore **/*.css",
    "prepare": "husky install",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:e2e": "cypress run --headless"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint",
    "*.css": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "1.15.1",
    "@nuxtjs/feed": "2.0.0",
    "@nuxtjs/sitemap": "2.4.0",
    "core-js": "3.21.1",
    "lodash": "4.17.21",
    "nuxt": "2.15.8",
    "vue-lazy-hydration": "2.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@babel/eslint-parser": "7.18.9",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "11.0.0",
    "@types/archiver": "5.3.1",
    "@types/lodash": "4.14.184",
    "@vue/test-utils": "1.3.0",
    "archiver": "5.3.1",
    "axios": "0.27.2",
    "babel-jest": "27.5.1",
    "basic-ftp": "4.6.6",
    "cypress": "10.7.0",
    "dotenv": "16.0.2",
    "eslint": "8.23.0",
    "eslint-plugin-nuxt": "3.2.0",
    "flush-promises": "1.0.2",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "lint-staged": "13.0.3",
    "stylelint": "14.11.0",
    "stylelint-config-standard": "28.0.0",
    "stylelint-order": "5.0.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.9.1",
    "typescript": "4.6.3",
    "vue-jest": "3.0.7"
  }
}
