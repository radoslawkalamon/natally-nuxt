{
  "name": "natally-nuxt",
  "version": "3.1.3",
  "private": true,
  "scripts": {
    "bootstrap:component": "ts-node ./bootstrap.component.ts",
    "deploy:storybook": "npm run storybook:build && ts-node ./deploy.storybook.ts",
    "deploy:stage": "npm run generate && ts-node ./deploy.stage.ts",
    "deploy:prod": "npm run generate && ts-node ./deploy.prod.ts",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "storybook:run": "nuxt storybook",
    "storybook:build": "nuxt storybook build",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "1.15.1",
    "@nuxtjs/feed": "2.0.0",
    "@nuxtjs/sitemap": "2.4.0",
    "core-js": "3.20.2",
    "nuxt": "2.15.8",
    "vue-lazy-hydration": "2.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.16.5",
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/storybook": "4.2.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.178",
    "@vue/test-utils": "1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.4.5",
    "basic-ftp": "4.6.6",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-nuxt": "3.1.0",
    "eslint-plugin-vue": "8.2.0",
    "flush-promises": "1.0.2",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "lint-staged": "12.1.5",
    "lodash": "4.17.21",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "4.1.0",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "vue-jest": "3.0.7"
  }
}
