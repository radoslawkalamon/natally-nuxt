{
  "name": "natally-nuxt",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "bootstrap:component": "ts-node ./bootstrap.component.ts",
    "build": "nuxt build",
    "deploy:storybook": "npm run storybook:build && ts-node ./deploy.storybook.ts",
    "deploy:stage": "npm run generate && ts-node ./deploy.stage.ts",
    "deploy:prod": "npm run generate && ts-node ./deploy.prod.ts",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "start": "nuxt start",
    "storybook:run": "nuxt storybook",
    "storybook:build": "nuxt storybook build",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "core-js": "^3.15.1",
    "nuxt": "^2.15.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/storybook": "^4.1.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.175",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "basic-ftp": "^4.6.6",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "husky": "^6.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "postcss": "^8.3.6",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "vue-jest": "^3.0.7"
  }
}
