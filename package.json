{
  "name": "natally-nuxt",
  "version": "3.1.7",
  "scripts": {
    "bootstrap:component": "ts-node ./bootstrap.component.ts",
    "deploy:storybook": "npm run storybook:build && ts-node ./deploy.storybook.ts",
    "deploy:stage": "npm run generate && ts-node ./deploy.stage.ts",
    "deploy:prod": "npm run generate && ts-node ./deploy.prod.ts",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint --ignore-path .gitignore ./**/*.{js,ts,vue}",
    "lint:style": "stylelint --ignore-path .gitignore **/*.css",
    "prepare": "husky install",
    "storybook:run": "nuxt storybook",
    "storybook:build": "nuxt storybook build",
    "test": "npm run test:unit",
    "test:unit": "jest"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint",
    "*.css": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "1.15.1",
    "@nuxtjs/feed": "2.0.0",
    "@nuxtjs/sitemap": "2.4.0",
    "core-js": "3.21.1",
    "lodash": "4.17.21",
    "nuxt": "2.15.8",
    "vue-lazy-hydration": "2.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/eslint-parser": "7.17.0",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "9.0.0",
    "@nuxtjs/storybook": "4.3.0",
    "@types/lodash": "4.14.181",
    "@vue/test-utils": "1.3.0",
    "babel-jest": "27.5.1",
    "basic-ftp": "4.6.6",
    "dotenv": "16.0.0",
    "eslint": "8.12.0",
    "eslint-plugin-nuxt": "3.2.0",
    "flush-promises": "1.0.2",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "stylelint": "14.6.1",
    "stylelint-config-standard": "25.0.0",
    "stylelint-order": "5.0.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "vue-jest": "3.0.7"
  }
}
